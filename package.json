{
  "name": "api-app-polox",
  "version": "1.0.0",
  "description": "Node.js Serverless API para AWS Lambda com PostgreSQL RDS",
  "main": "src/handler.js",
  "scripts": {
    "dev": "serverless offline --stage dev",
    "dev:local": "nodemon src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:verbose": "jest --verbose",
    "test:validation": "jest tests/integration/company-validation.test.js",
    "test:performance": "jest tests/integration/performance.test.js",
    "test:relationships": "jest tests/integration/relationships.test.js",
    "test:schedule": "node scripts/test-schedule.js",
    "test:schedule:unit": "jest tests/unit/schedule-controller.test.js --verbose",
    "test:schedule:integration": "jest tests/integration/schedule-controller.test.js --verbose",
    "deploy:dev": "serverless deploy --stage dev",
    "deploy:sandbox": "serverless deploy --stage sandbox",
    "deploy:prod": "serverless deploy --stage prod",
    "remove:dev": "serverless remove --stage dev",
    "remove:sandbox": "serverless remove --stage sandbox",
    "remove:prod": "serverless remove --stage prod",
    "logs:dev": "serverless logs -f api --stage dev --tail",
    "logs:sandbox": "serverless logs -f api --stage sandbox --tail",
    "logs:prod": "serverless logs -f api --stage prod --tail",
    "migrate": "node migrations/migration-runner.js up",
    "migrate:dev": "node scripts/run-migrations-dev.js",
    "migrate:rollback": "node migrations/migration-runner.js down",
    "migrate:status": "node migrations/migration-runner.js status",
    "migrate:create": "node scripts/create-migration.js",
    "migrate:dev:status": "node scripts/migrate-environment.js dev status",
    "migrate:dev:run": "node scripts/migrate-environment.js dev migrate",
    "migrate:dev:rollback": "node scripts/migrate-environment.js dev rollback",
    "migrate:sandbox:status": "node scripts/migrate-environment.js sandbox status",
    "migrate:sandbox": "node scripts/migrate-environment.js sandbox migrate",
    "migrate:sandbox:rollback": "node scripts/migrate-environment.js sandbox rollback",
    "migrate:prod:status": "node scripts/migrate-environment.js prod status",
    "migrate:prod": "node scripts/migrate-environment.js prod migrate",
    "migrate:test:status": "node scripts/migrate-test.js status",
    "migrate:test": "node scripts/migrate-test.js migrate",
    "migrate:test:rollback": "node scripts/migrate-test.js rollback",
    "migrate:check-all": "node scripts/check-all-environments.js",
    "setup:ssl": "node -e \"process.platform === 'win32' ? require('child_process').execSync('powershell -ExecutionPolicy Bypass -File scripts/setup-ssl.ps1', {stdio: 'inherit'}) : require('child_process').execSync('bash scripts/setup-ssl.sh', {stdio: 'inherit'})\"",
    "db:seed": "node scripts/seed-database.js",
    "db:reset": "node scripts/reset-database.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "security:audit": "npm audit",
    "security:check": "node scripts/security-check.js",
    "copyright:add": "node scripts/add-copyright-headers.js",
    "copyright:validate": "node src/middleware/copyright-validator.js",
    "git-hooks:setup": "node scripts/setup-git-hooks.js",
    "postinstall": "npm run setup:ssl && npm run git-hooks:setup"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.914.0",
    "aws-sdk": "^2.1481.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.5",
    "express-slow-down": "^2.0.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "i18next": "^25.6.0",
    "i18next-fs-backend": "^2.6.0",
    "i18next-http-middleware": "^3.8.1",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "node-cron": "^3.0.2",
    "node-fetch": "^3.3.2",
    "pg": "^8.11.3",
    "pg-pool": "^3.6.1",
    "prom-client": "^13.2.0",
    "redis": "^4.6.8",
    "serverless-http": "^4.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.1.0",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.8.10",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "serverless": "^3.38.0",
    "serverless-bundle": "^6.3.1",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-esbuild": "^1.55.1",
    "serverless-offline": "^13.2.0",
    "supertest": "^6.3.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "serverless",
    "aws",
    "lambda",
    "postgresql",
    "rds",
    "api",
    "node.js"
  ],
  "author": "Polox Team",
  "license": "MIT"
}
